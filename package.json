{
  "name": "brewday",
  "version": "1.0.0",
  "description": "MERN application for tracking your homebrew",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
